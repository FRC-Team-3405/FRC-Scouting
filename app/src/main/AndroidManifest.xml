<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sub6resources.frcscouting">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".App">

        <activity android:name="com.sub6resources.frcscouting.MainActivity" />
        <activity android:name=".form.FormCreateActivity" />
        <activity android:name=".form.FormActivity" />
        <activity android:name=".login.LoginActivity" android:fitsSystemWindows="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".formresponse.ResponseActivity"
            android:parentActivityName=".MainActivity" />

    </application>

</manifest>
